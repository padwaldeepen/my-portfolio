{
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "eslint.validate": [
    "javascript",
    "javascriptReact",
    "html",
    "typescriptreact",
    "typescript"
  ],
  "eslint.format.enable": true,
  "editor.tabSize": 2,
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  }
}